<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    package="com.visitore.visitoreclient">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".login.LoginInit"
            android:label="@string/title_activity_login_init"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <!--intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter-->
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name=".menuPrincipal.MenuPrincipal"
            android:screenOrientation="portrait" />
        <activity
            android:name=".PantallaPrincipal.PantallaPrincipalActivity"
            android:label="@string/title_activity_pantalla_principal"
            android:theme="@style/MyMaterialTheme"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".loader.LoadingIncialActivity"
            android:label="@string/title_activity_login_init"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>